@{
}
@model Tuple<EmpresaCadetes.ViewModels.CadeteAltaViewModel,EmpresaCadetes.ViewModels.UsuarioViewModel>
@await Html.PartialAsync("_NavAdminPartial",Model.Item2)
<h2 class=align-content-center>Porfavor Ingrese los datos del Cadete</h2>
<form asp-action="FormularioCadete" asp-controller="Cadeteria" method="post">
    <div class="form-group">
        <label for="formGroupExampleInput">Nombre</label>
        <input required type="text" name="nombre" class="form-control" id="nombre">
    </div>

    <div class="form-group">
        <label for="formGroupExampleInput2">Direccion</label>
        <input required type="text" name="direcion" class="form-control" id="direcion">
    </div>

    <div class="form-group">
        <label for="formGroupExampleInput2">Telefono</label>
        <input required type="text" name="telefono" class="form-control" id="telefono">
    </div>

    <input type="submit" name="btn" value="Cargar Cadete" />
</form>